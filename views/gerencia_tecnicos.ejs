    <!--==========================
      Modalidades
    ============================-->
    <section id="atleta">
      <div class="container">
        <div class="section-header wow fadeInUp">
          <h3>Técnicos</h3>
          <p>Gerencie um técnico para uma modalidade</p>
        </div>

        <form class="form-horizontal" action="/gerencia_tecnicos" method="post" align="center" enctype="multipart/form-data">
    
        <div class="form-group" align="center">
          <label class="control-label col-sm-2" for="modalidade">Modalidade:</label>
          <div class="col-sm-10">          
            <select type="select" class="form-control" id="modalidade" name="modalidade">
              <% for(var modalidade=0; modalidade < modalidades.length; modalidade++) { %>
              <option value="<%=modalidades[modalidade].Nome%>"><%=modalidades[modalidade].Nome%></option>
              <%}%>
            </select>
          </div>
        </div>

        <div class="form-group" align="center">
          <label class="control-label col-sm-2" for="id_tecnico">Técnico:</label>
          <div class="col-sm-10">          
            <select type="select" class="form-control" id="id_tecnico" name="id_tecnico">
              <% for(var tecnico=0; tecnico < tecnicos.length; tecnico++) { %>
              <option value="<%=tecnicos[tecnico].ID_Tecnico%>"><%=tecnicos[tecnico].Nome%></option>
              <%}%>
            </select>
          </div>
        </div>

        <div class="form-group" align="center">
          <label class="control-label col-sm-2" for="salario">Salário:</label>
          <div class="col-sm-10">          
            <input type="text" class="form-control" id="salario" name="salario">
          </div>
          </div>
        </div>
    
        <div class="form-group" align="center">        
          <div class="col-sm-offset-2 col-sm-10">
            <input  name="submit" type="submit" class="btn btn-default" value="Adicionar Tecnico à modalidade"></input>
          </div>
        </div>
    </form>
  </div>
  </section><!-- #modalidades -->

